<?xml version="1.0" encoding="UTF-8"?>
<suite name="Nopcommerce testsuite" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="report.config.MethodListener"></listener>
<!--        <listener class-name="commons.RetryListener"/>-->
        <listener class-name="report.config.ExtentTestListener"/>
    </listeners>
    <parameter name="userSiteUrl" value="https://demo.nopcommerce.com/"></parameter>
    <parameter name="adminSiteLoginUrl" value="https://admin-demo.nopcommerce.com/"></parameter>
    <parameter name="adminSiteDashboardUrl" value="https://admin-demo.nopcommerce.com/Admin/"></parameter>

    <test name="Chrome" description="Run on chrome">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="browserVersion" value="latest"></parameter>
        <parameter name="environmentName" value="local"></parameter>
        <parameter name="ipAddress" value="localhost"></parameter>
        <parameter name="port" value="4444"></parameter>
        <parameter name="os" value="Windows"></parameter>
        <parameter name="osVersion" value="10"></parameter>
        <classes>
            <class name="admin.PreCondition_Login"></class>
            <class name="admin.catalog.product.Search"></class>
        </classes>
    </test>

